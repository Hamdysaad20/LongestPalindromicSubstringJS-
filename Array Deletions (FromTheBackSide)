import java.util.Arrays;
public class GFG
{
	public static void main (String[] args)
	{
           
int arr1[] = {1,2,309,66,0};
 int arr1ln =arr1.length-1;
int[] newArray = new int[arr1ln];
for(int i=0;i<arr1ln;i++){
     newArray[i]=arr1[i];
}
        
        System.out.println(Arrays.toString(newArray));


}
}
